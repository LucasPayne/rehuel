
set grid
set log xy
set format x "10^{%T}"
set format y "10^{%T}"
set xrange[1e-3:1e3]
set yrange[1e-12:1e4]
set key bottom right

plot 'errs_RADAU_IIA_32.dat' w p pt 4 ps 2 lw 2 ti 'Radau IIA 3(2)' \
   , 'errs_RADAU_IIA_53.dat' w p pt 4 ps 2 lw 2 ti 'Radau IIA 5(3)' \
   , 'errs_RADAU_IIA_95.dat' w p pt 4 ps 2 lw 2 ti 'Radau IIA 9(5)' \
   , 1e1*x**3 w l lw 2 dt 2 ti '$\sim x^3$' \
   , 4e-3*x**5 w l lw 2 dt 2 ti '$\sim x^5$' \
   , 3e-10*x**9 w l lw 2 dt 2 ti '$\sim x^9$' \
